<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kwon.jun191smmb.menu.MenuMapper">
	<insert id="reg" 
		parameterType="com.kwon.jun191smmb.menu.Menu">
		insert into jun19_menu 
		values(#{m_name}, #{m_price})
	</insert>
	<select id="get" 
		resultType="com.kwon.jun191smmb.menu.Menu">
		select * from jun19_menu order by m_name
	</select>
</mapper>

