<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kwon.jul072rac.coffee.CoffeeMapper">
	<select id="get" resultType="com.kwon.jul072rac.coffee.Coffee">
		select * from jul07_coffee order by c_name
	</select>
	<insert id="reg" parameterType="com.kwon.jul072rac.coffee.Coffee">
		insert into jul07_coffee values(#{c_name}, #{c_price})
	</insert>
</mapper>